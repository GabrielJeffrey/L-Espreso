{
  "name": "la-espresso",
  "version": "1.0.0",
  "description": "An Online Food Services Restaurant Built By and For Gabriel's Portfolio Purpose",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start:prod": "SET NODE_ENV=production&nodemon server.js",
    "debug": "ndb server.js",
    "seed-DB": "node dev-data/seed-dev-data.js --seed",
    "delete-DB": "node dev-data/seed-dev-data.js --delete",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Gabriel Jeffrey",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.6.1",
    "helmet": "^4.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.28",
    "morgan": "^1.10.0",
    "pug": "^3.0.0",
    "slugify": "^1.4.5",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "engines": {
    "node": "^10"
  }
}
